{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport TutorialDataService from \"../services/TutorialDataService\";\nexport default {\n  name: \"tutorial-detail\",\n  data() {\n    return {\n      guncellenecekTutorial: null\n    };\n  },\n  methods: {\n    tutorialDetayGetir(id) {\n      TutorialDataService.getTutorialDetail(id).then(gelenTutorial => {\n        this.guncellenecekTutorial = gelenTutorial.data;\n      });\n    },\n    tutorialGuncelle() {\n      console.log(\"guncellenecekTutorial id: \" + this.guncellenecekTutorial.id);\n      console.log(\"guncellenecekTutorial data: \" + this.guncellenecekTutorial);\n      TutorialDataService.updateTutorial(this.guncellenecekTutorial.id, this.guncellenecekTutorial).then(guncellenmisTutorial => {\n        console.log(guncellenmisTutorial.data);\n      }).catch(hata => {\n        console.log(\"hata oluştu :\" + hata);\n      });\n    },\n    tutorialSil() {\n      if (confirm(\"Silmek istediğinizden emin misiniz\")) {\n        TutorialDataService.deleteTutorial(this.guncellenecekTutorial.id).then(response => {\n          console.log(response);\n          this.$router.push({\n            name: \"tutorials\"\n          });\n        }).catch(error => {\n          console.log(error);\n        });\n      } else {\n        console.log(\"Silme Gerçekleşmedi\");\n      }\n    }\n  },\n  mounted() {\n    //bana bir önceki yani tutorialListteki tıklanmış olan tutorialın id'si gelicek\n    this.tutorialDetayGetir(this.$route.params.id);\n  }\n};","map":{"version":3,"names":["TutorialDataService","name","data","guncellenecekTutorial","methods","tutorialDetayGetir","id","getTutorialDetail","then","gelenTutorial","tutorialGuncelle","console","log","updateTutorial","guncellenmisTutorial","catch","hata","tutorialSil","confirm","deleteTutorial","response","$router","push","error","mounted","$route","params"],"sources":["/Users/semayuceuluc/Downloads/TechCarrer/vue-crud-ornek/src/components/TutorialDetail.vue"],"sourcesContent":["<template>\n  <div v-if=\"guncellenecekTutorial\" class=\"submit-form\">\n    <div class=\"form-group\">\n      <label for=\"title\"> Başlık </label>\n      <input type=\"text\" class=\"form-control\" id=\"title\" v-model=\"guncellenecekTutorial.title\" required name=\"title\" />\n    </div>\n    <div class=\"form-group\">\n      <label for=\"description\"> Açıklama </label>\n      <input type=\"text\" class=\"form-control\" id=\"description\" v-model=\"guncellenecekTutorial.description\" required name=\"description\" />\n    </div>\n\n    <br />\n\n<button type=\"button\" class=\"btn btn-success\" @click=\"tutorialGuncelle\">Güncelle </button>\n<button type=\"button\" class=\"btn btn-danger\" @click=\"tutorialSil\" onclick=\"return confirm('Silmek istediğinizden emin misiniz?');\">Sil </button>\n\n  </div>\n</template>\n\n<script>\nimport TutorialDataService from \"../services/TutorialDataService\"\nexport default {\n  name: \"tutorial-detail\",\n  data() {\n    return {\n      guncellenecekTutorial: null,\n    }\n  },\n  methods: {\n    tutorialDetayGetir(id) {\n      TutorialDataService.getTutorialDetail(id).then(gelenTutorial => {\n        this.guncellenecekTutorial = gelenTutorial.data;\n      })\n    },\n    tutorialGuncelle(){\n        console.log(\"guncellenecekTutorial id: \" +this.guncellenecekTutorial.id);\n        console.log(\"guncellenecekTutorial data: \" +this.guncellenecekTutorial);\n        TutorialDataService.updateTutorial(this.guncellenecekTutorial.id,this.guncellenecekTutorial).then(guncellenmisTutorial => {\n            console.log(guncellenmisTutorial.data);\n        }).catch(hata => {\n            console.log(\"hata oluştu :\" +hata);\n        })\n    },\n    tutorialSil(){\n   if(confirm(\"Silmek istediğinizden emin misiniz\")){\n       TutorialDataService.deleteTutorial(this.guncellenecekTutorial.id).then(response => {\n           console.log(response);\n           this.$router.push({ name: \"tutorials\"});\n       }).catch(error => {\n           console.log(error);\n       });\n   } else {\n       console.log(\"Silme Gerçekleşmedi\");\n   }\n}\n\n  },\n  mounted() {\n    //bana bir önceki yani tutorialListteki tıklanmış olan tutorialın id'si gelicek\n    this.tutorialDetayGetir(this.$route.params.id);\n  },\n}\n</script>\n"],"mappings":";AAoBA,OAAOA,mBAAkB,MAAO,iCAAgC;AAChE,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,qBAAqB,EAAE;IACzB;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,kBAAkBA,CAACC,EAAE,EAAE;MACrBN,mBAAmB,CAACO,iBAAiB,CAACD,EAAE,CAAC,CAACE,IAAI,CAACC,aAAY,IAAK;QAC9D,IAAI,CAACN,qBAAoB,GAAIM,aAAa,CAACP,IAAI;MACjD,CAAC;IACH,CAAC;IACDQ,gBAAgBA,CAAA,EAAE;MACdC,OAAO,CAACC,GAAG,CAAC,4BAA2B,GAAG,IAAI,CAACT,qBAAqB,CAACG,EAAE,CAAC;MACxEK,OAAO,CAACC,GAAG,CAAC,8BAA6B,GAAG,IAAI,CAACT,qBAAqB,CAAC;MACvEH,mBAAmB,CAACa,cAAc,CAAC,IAAI,CAACV,qBAAqB,CAACG,EAAE,EAAC,IAAI,CAACH,qBAAqB,CAAC,CAACK,IAAI,CAACM,oBAAmB,IAAK;QACtHH,OAAO,CAACC,GAAG,CAACE,oBAAoB,CAACZ,IAAI,CAAC;MAC1C,CAAC,CAAC,CAACa,KAAK,CAACC,IAAG,IAAK;QACbL,OAAO,CAACC,GAAG,CAAC,eAAc,GAAGI,IAAI,CAAC;MACtC,CAAC;IACL,CAAC;IACDC,WAAWA,CAAA,EAAE;MACd,IAAGC,OAAO,CAAC,oCAAoC,CAAC,EAAC;QAC7ClB,mBAAmB,CAACmB,cAAc,CAAC,IAAI,CAAChB,qBAAqB,CAACG,EAAE,CAAC,CAACE,IAAI,CAACY,QAAO,IAAK;UAC/ET,OAAO,CAACC,GAAG,CAACQ,QAAQ,CAAC;UACrB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;YAAErB,IAAI,EAAE;UAAW,CAAC,CAAC;QAC3C,CAAC,CAAC,CAACc,KAAK,CAACQ,KAAI,IAAK;UACdZ,OAAO,CAACC,GAAG,CAACW,KAAK,CAAC;QACtB,CAAC,CAAC;MACN,OAAO;QACHZ,OAAO,CAACC,GAAG,CAAC,qBAAqB,CAAC;MACtC;IACH;EAEE,CAAC;EACDY,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACnB,kBAAkB,CAAC,IAAI,CAACoB,MAAM,CAACC,MAAM,CAACpB,EAAE,CAAC;EAChD;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}